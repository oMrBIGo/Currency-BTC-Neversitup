        <font fgcolor='#8BC34A'>/*** set variable n = 10 ***/</font>\n
        \n
        <font fgcolor='#bc7331'>fun</font> <font fgcolor='#eabd66'>fibonacci</font>(n: Int): String {\n
        \n
        \t\t<font fgcolor='#bc7331'>val</font> result = LongArray(n)\n
        \t\tresult[<font fgcolor='#4e7593'>1</font>] = <font fgcolor='#4e7593'>1</font>\n
        \n
        \t\t<font fgcolor='#bc7331'>for</font> (i <font fgcolor='#bc7331'>in</font> <font fgcolor='#4e7593'>2</font> <font fgcolor='#eabd66'>until</font> n) {\n
        \t\t\t\tresult[i] = result[i - <font fgcolor='#4e7593'>1</font>] + result[i - <font fgcolor='#4e7593'>2</font>]\n
        \t\t}\n
        \n
        \t\t<font fgcolor='#bc7331'>return</font> result.<font fgcolor='#eabd66'>contentToString</font>()\n
        }
    </string>

